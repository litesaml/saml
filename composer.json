{
  "name": "litesaml/saml",
  "description": "SAML 2.0 PHP library",
  "keywords": ["SAML 2.0", "PHP", "library", "lightSAML", "Single SignOn", "Single Logout"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "William",
      "email": "work@suppo.fr"
    }
  ],
  "require": {
    "php": "^8.2",
    "bluestone/dto": "^1.1",
    "litesaml/lightsaml": "^4.2",
    "symfony/http-foundation": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "mockery/mockery": "^1.6",
    "squizlabs/php_codesniffer": "^3.9"
  },
  "autoload": {
    "psr-4": {
      "Litesaml\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
